<!DOCTYPE html>
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel='stylesheet' href="/stylesheets/style.css"
  <title></title>
</head>


<body>
    <h1>Skills</h1>
    <ul>
      <% skills.forEach(function(s) { %>
        <li>
            <form action="/skills/<%= s.id %>?_method=DELETE"
              class="delete-form" method="POST">
              <button type="submit">X<br></button>
            </form>
            <!-- <a href="/skills/<%= s.id %>"<%= s.skill%></a> -->
            <%= s.id %>

          <%= s.skill %>
            - 
          <%= s.done ? 'done' : 'not done' %>
        </li>
      <% }); %>
    </ul>
    <a href="/skills/new">Add Skill</a>
  </body>
  </html>